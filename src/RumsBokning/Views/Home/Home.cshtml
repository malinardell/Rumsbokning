@model HomeVM
    <div id="all">
        <center>
            <div class="dropdown">
                <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">
                    Grupprum
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    @foreach (var item in Model.RoomItems)
                    {
                        <li><a href="javascript:getCalendar(@item.Value)">@item.Text</a></li>
                    }
                </ul>
            </div>

            <div class="container">
                <hr>
                <div id="calendar">
                </div>
            </div>
        </center>
</div>
